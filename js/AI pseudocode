update() {
		
	If (no flag) {
		task = get flag
	} else if (flag) {
		task = go home
	}

	if (task == get flag && no path) {
		generate a path
	} else if (task == go home && no path) {
		generate a path
	}



	while (the path list is not empty) {
		move slowly to the next designated spot
		if reached {
			pop it off
		}
	}




}